INSERT INTO `phpmx_locales_source` VALUES ('2022',' modules/user/user.admin.inc:235','default','If this box is checked, new users will be required to validate their e-mail address prior to logging into the site, and will be assigned a system-generated password. With it unchecked, users will be logged in immediately upon registering, and may select their own passwords during registration.','none'),('2023',' modules/user/user.admin.inc:236','default','User registration guidelines','none'),('2024',' modules/user/user.admin.inc:236','default','This text is displayed at the top of the user registration form and is useful for helping or instructing your users.','none'),('2025',' modules/user/user.admin.inc:242','default','Drupal sends emails whenever new users register on your site, and optionally, may also notify users after other account actions. Using a simple set of content templates, notification e-mails can be customized to fit the specific needs of your site.','none'),('2026',' modules/user/user.admin.inc:250','default','Welcome, new user created by administrator','none'),('2027',' modules/user/user.admin.inc:253','default','Customize welcome e-mail messages sent to new member accounts created by an administrator.','none'),('2028',' modules/user/user.admin.inc:270','default','Welcome, no approval required','none'),('2029',' modules/user/user.admin.inc:273','default','Customize welcome e-mail messages sent to new members upon registering, when no administrator approval is required.','none'),('2030',' modules/user/user.admin.inc:290','default','Welcome, awaiting administrator approval','none'),('2031',' modules/user/user.admin.inc:293','default','Customize welcome e-mail messages sent to new members upon registering, when administrative approval is required.','none'),('2032',' modules/user/user.admin.inc:310','default','Password recovery email','none'),('2033',' modules/user/user.admin.inc:313','default','Customize e-mail messages sent to users who request a new pass