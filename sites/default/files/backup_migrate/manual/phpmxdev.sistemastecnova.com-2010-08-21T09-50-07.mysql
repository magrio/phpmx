INSERT INTO `phpmx_locales_source` VALUES ('2505',' modules/contact/contact.admin.inc:92','default','%recipient is an invalid e-mail address.','none'),('2506',' modules/contact/contact.admin.inc:156','default','Additional information','none'),('2507',' modules/contact/contact.admin.inc:164','default','The maximum number of contact form submissions a user can perform per hour.','none'),('2508',' modules/contact/contact.admin.inc:115','default','Contact form: category %category added.','none'),('2509',' modules/contact/contact.admin.inc:121','default','Contact form: category %category updated.','none'),('2510',' modules/contact/contact.admin.inc:168','default','Enable personal contact form by default','none'),('2511',' modules/contact/contact.admin.inc:170','default','Default status of the personal contact form for new users.','none'),('2512',' modules/contact/contact.admin.inc:158','default','Information to show on the <a href=\"@form\">contact page</a>. Can be anything from submission guidelines to your postal address or telephone number.','none'),('2513',' modules/contact/contact.pages.inc:161','default','You need to provide a valid e-mail address to contact other users. Please update your <a href=\"@url\">user information</a> and try again.','none'),('2514',' modules/contact/contact.pages.inc:164','default','You cannot contact more than %number users per hour. Please try again later.','none'),('2515',' modules/contact/contact.module:46','default','Contact form','6.19'),('2516',' modules/contact/contact.admin.inc:43','default','Example: \'website feedback\' or \'product information\'.','none'),('2517',' modules/contact/contact.admin.inc:60','default','When listing categories, those with lighter (smaller) weights get listed before categories with heavier (larger) weights. Categories with equal weights are sorted alphabetically.','none'),('2518',' modules/contact/contact.admin.inc:148,148','default','Contact form: category %category deleted.','none'),('2519',' module