INSERT INTO `phpmx_locales_source` VALUES ('920',' modules/system/system.install:290','default','HTTP request status','none'),('921',' modules/system/system.install:291','default','Fails','none'),('922',' modules/system/system.install:293','default','Your system or network configuration does not allow Drupal to access web pages, resulting in reduced functionality. This could be due to your webserver configuration or PHP settings, and should be resolved in order to download information about available updates, fetch aggregator feeds, sign in via OpenID, or use other network-dependent services.','none'),('923',' modules/system/system.admin.inc:167','default','Choose which theme the administration pages should display in. If you choose \"System default\" the administration pages will use the same theme as the rest of the site.','none'),('924',' modules/system/system.admin.inc:295','default','Please note that the <a href=\"!admin_theme_page\">administration theme</a> is still set to the %admin_theme theme; consequently, the theme on this page remains unchanged. All non-administrative sections of the site, however, will show the selected %selected_theme theme by default.','none'),('925',' modules/system/system.admin.inc:388','default','User pictures in posts','6.19'),('926',' modules/system/system.admin.inc:389','default','User pictures in comments','6.19'),('927',' modules/system/system.admin.inc:391','default','Shortcut icon','6.19'),('928',' modules/system/system.admin.inc:434','default','Display post information on','none'),('929',' modules/system/system.admin.inc:435','default','Enable or disable the <em>submitted by Username on date</em> text when displaying posts of the following type.','none'),('930',' modules/system/system.admin.inc:454','default','Logo image settings','6.19'),('931',' modules/system/system.admin.inc:455','default','If toggled on, the following logo will be displayed.','6.19'),('932',' modules/system/system.admin.inc:460','default','Use the defa