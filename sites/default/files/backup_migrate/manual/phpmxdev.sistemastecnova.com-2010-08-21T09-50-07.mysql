INSERT INTO `phpmx_locales_source` VALUES ('1750',' modules/node/node.admin.inc:18','default','Node access status','none'),('1751',' modules/node/node.admin.inc:28','default','Number of posts on main page','none'),('1752',' modules/node/node.admin.inc:30','default','The default maximum number of posts to display per page on overview pages such as the main page.','none'),('1753',' modules/node/node.admin.inc:72','default','Are you sure you want to rebuild the permissions on site content?','none'),('1754',' modules/node/node.admin.inc:73','default','This action rebuilds all permissions on site content, and may be a lengthy process. This action cannot be undone.','none'),('1755',' modules/node/node.admin.inc:105','default','Demote from front page','none'),('1756',' modules/node/node.admin.inc:110','default','Make sticky','none'),('1757',' modules/node/node.admin.inc:115','default','Remove stickiness','none'),('1758',' modules/node/node.admin.inc:160','default','language','none'),('1759',' modules/node/node.admin.inc:427','default','An error occurred and processing did not complete.','none'),('1760',' modules/node/node.pages.inc:284','default','Show summary in full view','6.19'),('1761',' modules/node/node.pages.inc:437','default','The trimmed version of your post shows what your post looks like when promoted to the main page or when exported for syndication.<span class=\"no-js\"> You can insert the delimiter \"&lt;!--break--&gt;\" (without the quotes) to fine-tune where your post gets split.</span>','none'),('1762',' modules/node/node.pages.inc:580','default','Copy of the revision from %date.','none'),('1763',' modules/node/node.pages.inc:588','default','@type %title has been reverted back to the revision from %revision-date.','none'),('1764',' modules/node/node.pages.inc:602','default','Revision from %revision-date of @type %title has been deleted.','none'),('1765',' modules/node/node.pages.inc:587','default','@type: reverted %title revision %revision.','none'),('1766