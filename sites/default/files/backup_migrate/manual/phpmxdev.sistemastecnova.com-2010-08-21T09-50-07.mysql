INSERT INTO `phpmx_locales_source` VALUES ('2295',' modules/aggregator/aggregator.module:145','default','Sources','6.19'),('2296',' modules/aggregator/aggregator.module:306','default','!title category latest items','6.19'),('2297',' modules/aggregator/aggregator.module:310','default','!title feed latest items','6.19'),('2298',' modules/aggregator/aggregator.admin.inc:295','default','A category named %category already exists. Please enter a unique title.','none'),('2299',' modules/aggregator/aggregator.admin.inc:314','default','The category %category has been updated.','none'),('2300',' modules/aggregator/aggregator.admin.inc:325','default','Category %category deleted.','none'),('2301',' modules/aggregator/aggregator.admin.inc:326','default','The category %category has been deleted.','none'),('2302',' modules/aggregator/aggregator.admin.inc:338','default','Category %category added.','none'),('2303',' modules/aggregator/aggregator.admin.inc:339','default','The category %category has been added.','6.19'),('2304',' modules/aggregator/aggregator.pages.inc:67','default','aggregator - @title','none'),('2305',' modules/aggregator/aggregator.module:717','default','The feed from %site seems to be broken, due to an error \"%error\" on line %line.','none'),('2306',' modules/aggregator/aggregator.module:718','default','The feed from %site seems to be broken, because of error \"%error\" on line %line.','none'),('2307',' modules/aggregator/aggregator.admin.inc:240','default','Discard items older than','none'),('2308',' modules/aggregator/aggregator.admin.inc:242','default','The length of time to retain feed items before discarding. (Requires a correctly configured <a href=\"@cron\">cron maintenance task</a>.)','none'),('2309',' modules/aggregator/aggregator.admin.inc:236','default','Number of feed items displayed in feed and category summary pages.','none'),('2310',' modules/aggregator/aggregator.pages.inc:376','default','@site_name - aggregated feeds in category @title','none'),(