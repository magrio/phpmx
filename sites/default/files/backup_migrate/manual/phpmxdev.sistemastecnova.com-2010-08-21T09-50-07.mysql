INSERT INTO `phpmx_locales_source` VALUES ('2312',' modules/aggregator/aggregator.pages.inc:380','default','@site_name aggregator','none'),('2313',' modules/aggregator/aggregator.pages.inc:476','default','!title feed','none'),('2314',' modules/aggregator/aggregator.module:15','default','The aggregator is a powerful on-site syndicator and news reader that gathers fresh content from RSS-, RDF-, and Atom-based feeds made available across the web. Thousands of sites (particularly news sites and blogs) publish their latest headlines and posts in feeds, using a number of standardized XML-based formats. Formats supported by the aggregator include <a href=\"@rss\">RSS</a>, <a href=\"@rdf\">RDF</a>, and <a href=\"@atom\">Atom</a>.','6.19'),('2315',' modules/aggregator/aggregator.module:16','default','Feeds contain feed items, or individual posts published by the site providing the feed. Feeds may be grouped in categories, generally by topic. Users view feed items in the <a href=\"@aggregator\">main aggregator display</a> or by <a href=\"@aggregator-sources\">their source</a>. Administrators can <a href=\"@feededit\">add, edit and delete feeds</a> and choose how often to check each feed for newly updated items. The most recent items in either a feed or category can be displayed as a block through the <a href=\"@admin-block\">blocks administration page</a>. A <a href=\"@aggregator-opml\">machine-readable OPML file</a> of all feeds is available. A correctly configured <a href=\"@cron\">cron maintenance task</a> is required to update feeds automatically.','6.19'),('2316',' modules/aggregator/aggregator.module:17','default','For more information, see the online handbook entry for <a href=\"@aggregator\">Aggregator module</a>.','6.19'),('2317',' modules/aggregator/aggregator.module:649','default','The feed from %site seems to be broken, because of error \"%error\".','none'),('2318',' modules/aggregator/aggregator.module:603','default','There is no new syndicated content from %site.','none'),('2319',' modules/aggregator/aggregator.module:642,641','default','There is new syndicated content from %site.','6.19');
INSERT INTO `phpmx_locales_source` VALUES ('2320',' modules/aggregator/aggregator.module:648','default','The feed from %site seems to be broken, due to \"%error\".','none'),('2321',' modules/aggregator/aggregator.admin.inc:80','default','The length of time between feed updates. (Requires a correctly configured <a href=\"@cron\">cron maintenance task</a>.)','6.19'),('2322',' modules/aggregator/aggregator.admin.inc:96','default','New feed items are automatically filed in the checked categories.','6.19'),('2323',' modules/aggregator/aggregator.admin.inc:116','default','The URL %url is invalid. Please enter a fully-qualified URL, such as http://www.example.com/feed.xml.','none'),('2324',' modules/aggregator/aggregator.module:90','default','Add feed','6.19'),('2325',' modules/aggregator/aggregator.module:239','default','Edit category','none'),('2326',' modules/aggregator/aggregator.admin.inc:66','default','The name of the feed (or the name of the website providing the feed).','6.19'),('2327',' modules/aggregator/aggregator.admin.inc:93','default','Categorize news items','6.19'),('2328',' modules/aggregator/aggregator.admin.inc:127','default','A feed named %feed already exists. Please enter a unique title.','none'),('2329',' modules/aggregator/aggregator.admin.inc:187','default','Are you sure you want to remove all items from the feed %feed?','none'),('2330',' modules/aggregator/aggregator.admin.inc:230','default','A space-separated list of HTML tags allowed in the content of feed items. (Tags in this list are not removed by Drupal.)','none'),('2331',' modules/aggregator/aggregator.admin.inc:246','default','Category selection type','none'),('2332',' modules/aggregator/aggregator.admin.inc:247','default','checkboxes','none'),('2333',' modules/aggregator/aggregator.admin.inc:247','default','multiple selector','none'),('2334',' modules/aggregator/aggregator.admin.inc:248','default','The type of category selection widget displayed on categorization pages. (For a small number of categories, checkboxes are easier to use, while a multiple selector work well with large numbers of categories.)','none');
INSERT INTO `phpmx_locales_source` VALUES ('2335',' modules/aggregator/aggregator.module:20','default','Thousands of sites (particularly news sites and blogs) publish their latest headlines and posts in feeds, using a number of standardized XML-based formats. Formats supported by the aggregator include <a href=\"@rss\">RSS</a>, <a href=\"@rdf\">RDF</a>, and <a href=\"@atom\">Atom</a>.','6.19'),('2336',' modules/aggregator/aggregator.module:21','default','Current feeds are listed below, and <a href=\"@addfeed\">new feeds may be added</a>. For each feed or feed category, the <em>latest items</em> block may be enabled at the <a href=\"@block\">blocks administration page</a>.','6.19'),('2337',' modules/aggregator/aggregator.module:24','default','Add a feed in RSS, RDF or Atom format. A feed may only have one entry.','6.19'),('2338',' modules/aggregator/aggregator.module:26','default','Categories allow feed items from different feeds to be grouped together. For example, several sport-related feeds may belong to a category named <em>Sports</em>. Feed items may be grouped automatically (by selecting a category when creating or editing a feed) or manually (via the <em>Categorize</em> page available from feed item listings). Each category provides its own feed page and block.','6.19'),('2339',' modules/aggregator/aggregator.module:645','default','feed not parseable','none'),('2340',' modules/aggregator/aggregator.module:84','default','Configure which content your site aggregates from other sites, how often it polls them, and how they\'re categorized.','6.19'),('2341',' modules/aggregator/aggregator.module:116','default','Update items','6.19'),('2342',' modules/aggregator/aggregator.module:231','default','Edit feed','6.19'),('2343',' modules/blog/blog.pages.inc:81','default','RSS - blogs','none'),('2344',' modules/blog/blog.module:146','default','My blog','none'),('2345',' modules/blog/blog.pages.inc:47','default','RSS - !title','none'),('2346',' modules/blog/blog.pages.inc:15','default','@name\'s blog','none'),('2347',' modules/blog/blog.module:198,207','default','Recent blog posts','6.19');
INSERT INTO `phpmx_locales_source` VALUES ('2348',' modules/blog/blog.pages.inc:20','default','Post new blog entry.','none'),('2349',' modules/blog/blog.pages.inc:23','default','You are not allowed to post a new blog entry.','none'),('2350',' modules/blog/blog.pages.inc:41','default','You have not created any blog entries.','none'),('2351',' modules/blog/blog.pages.inc:44','default','!author has not created any blog entries.','none'),('2352',' modules/blog/blog.pages.inc:62','default','Create new blog entry.','none'),('2353',' modules/blog/blog.pages.inc:79','default','No blog entries have been created.','none'),('2354',' modules/blog/blog.module:53','default','View recent blog entries','none'),('2355',' modules/blog/blog.module:53,126','default','Read !username\'s latest blog entries.','none'),('2356',' modules/blog/blog.module:65','default','The blog module allows registered users to maintain an online journal, or <em>blog</em>. Blogs are made up of individual <em>blog entries</em>, and the blog entries are most often displayed in descending order by creation time.','none'),('2357',' modules/blog/blog.module:66','default','There is an (optional) <em>Blogs</em> menu item added to the Navigation menu, which displays all blogs available on your site, and a <em>My blog</em> item displaying the current user\'s blog entries. The <em>Blog entry</em> menu item under <em>Create content</em> allows new blog entries to be created.','none'),('2358',' modules/blog/blog.module:67','default','Each blog entry is displayed with an automatic link to other blogs created by the same user. By default, blog entries have comments enabled and are automatically promoted to the site front page. The blog module also creates a <em>Recent blog posts</em> block that may be enabled at the <a href=\"@blocks\">blocks administration page</a>.','none'),('2359',' modules/blog/blog.module:68','default','When using the aggregator module an automatic <em>blog it</em> icon is displayed next to the items in a feed\'s <em>latest items</em> block. Clicking this icon populates a <em>blog entry</em> with a title (the title of the feed item) and body (a link to the source item on its original site and illustrative content suitable for use in a block quote). Blog authors can use this feature to easily comment on items of interest that appear in aggregator feeds from other sites. To use this feature, be sure to <a href=\"@modules\">enable</a> the aggregator module, <a href=\"@feeds\">add and configure</a> a feed from another site, and <a href=\"@blocks\">position</a> the feed\'s <em>latest items</em> block.','none');
INSERT INTO `phpmx_locales_source` VALUES ('2360',' modules/blog/blog.module:69','default','For more information, see the online handbook entry for <a href=\"@blog\">Blog module</a>.','none'),('2361',' modules/blog/blog.module:124','default','!username\'s blog','none'),('2362',' modules/blog/blog.module:206','default','Read the latest blog entries.','none'),('2363',' modules/blog/blog.module:26','default','create blog entries','none'),('2364',' modules/blog/blog.module:26','default','delete own blog entries','none'),('2365',' modules/blog/blog.module:26','default','delete any blog entry','none'),('2366',' modules/blog/blog.module:26','default','edit own blog entries','none'),('2367',' modules/blog/blog.module:26','default','edit any blog entry','none'),('2368',' modules/blog/blog.pages.inc:106,106','default','!site_name blogs','none'),('2369',' modules/blogapi/blogapi.module:684','default','Wrong username or password.','none'),('2370',' modules/blogapi/blogapi.module:785','default','The maximum size of all files a user can have on the site (in megabytes).','none'),('2371',' modules/blogapi/blogapi.module:777','default','The maximum size of a file a user can upload (in megabytes).','none'),('2372',' modules/blogapi/blogapi.module:720','default','File settings','none'),('2373',' modules/blogapi/blogapi.module:44','default','Returns information about an author in the system.','none'),('2374',' modules/blogapi/blogapi.module:49,74','default','Creates a new post, and optionally publishes it.','none'),('2375',' modules/blogapi/blogapi.module:54','default','Updates the information about an existing post.','none'),('2376',' modules/blogapi/blogapi.module:59,84','default','Returns information about a specific post.','none'),('2377',' modules/blogapi/blogapi.module:64','default','Deletes a post.','none'),('2378',' modules/blogapi/blogapi.module:69,99','default','Returns a list of the most recent posts in the system.','none'),('2379',' modules/blogapi/blogapi.module:79','defau