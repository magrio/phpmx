INSERT INTO `phpmx_locales_source` VALUES ('1533',' modules/locale/locale.module:0','default','!long-month-name November','none'),('1534',' modules/locale/locale.module:0','default','!long-month-name December','none'),('1535',' modules/menu/menu.module:96','default','Add item','none'),('1536',' modules/menu/menu.admin.inc:175','default','Menu item','none'),('1537',' modules/menu/menu.admin.inc:177,299','default','Expanded','none'),('1538',' modules/menu/menu.admin.inc:260','default','The path this menu item links to. This can be an internal Drupal path such as %add-node or an external URL such as %drupal. Enter %front to link to the front page.','none'),('1539',' modules/menu/menu.admin.inc:301','default','If selected and this menu item has children, the menu will always appear expanded.','none'),('1540',' modules/menu/menu.admin.inc:104','default','reset','none'),('1541',' modules/menu/menu.module:63','default','Add menu','none'),('1542',' modules/menu/menu.admin.inc:564','default','Are you sure you want to delete the custom menu item %item?','none'),('1543',' modules/menu/menu.admin.inc:574','default','The menu item %title has been deleted.','none'),('1544',' modules/menu/menu.admin.inc:575','default','Deleted menu item %title.','none'),('1545',' modules/menu/menu.module:104','default','Edit menu','none'),('1546',' modules/menu/menu.module:112','default','Delete menu','none'),('1547',' modules/menu/menu.module:120','default','Edit menu item','none'),('1548',' modules/menu/menu.module:128','default','Reset menu item','none'),('1549',' modules/menu/menu.module:136','default','Delete menu item','none'),('1550',' modules/menu/menu.admin.inc:289','default','The description displayed when hovering over a menu item.','none'),('1551',' modules/menu/menu.admin.inc:616','default','No primary links','none'),('1552',' modules/menu/menu.admin.inc:619','default','Source for the primary links','none'),('1553',' modules/menu/menu.admin.inc:623','default','Select what should be di