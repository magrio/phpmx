INSERT INTO `phpmx_locales_source` VALUES ('934',' modules/system/system.admin.inc:469','default','The path to the file you would like to use as your logo file instead of the default logo.','6.19'),('935',' modules/system/system.admin.inc:473','default','Upload logo image','6.19'),('936',' modules/system/system.admin.inc:482','default','Shortcut icon settings','6.19'),('937',' modules/system/system.admin.inc:483','default','Your shortcut icon, or \'favicon\', is displayed in the address bar and bookmarks of most browsers.','6.19'),('938',' modules/system/system.admin.inc:487','default','Use the default shortcut icon.','6.19'),('939',' modules/system/system.admin.inc:489','default','Check here if you want the theme to use the default shortcut icon.','6.19'),('940',' modules/system/system.admin.inc:495','default','The path to the image file you would like to use as your custom shortcut icon.','6.19'),('941',' modules/system/system.admin.inc:500','default','Upload icon image','6.19'),('942',' modules/system/system.admin.inc:501','default','If you don\'t have direct file access to the server, use this field to upload your shortcut icon.','6.19'),('943',' modules/system/system.admin.inc:689','default','@module (<span class=\"admin-missing\">missing</span>)','6.19'),('944',' modules/system/system.admin.inc:1082','default','The following modules will be completely uninstalled from your site, and <em>all data from these modules will be lost</em>!','none'),('945',' modules/system/system.admin.inc:1085','default','Confirm uninstall','none'),('946',' modules/system/system.admin.inc:1087','default','Would you like to continue with uninstalling the above?','none'),('947',' modules/system/system.admin.inc:1100','default','No modules selected.','none'),('948',' modules/system/system.admin.inc:1117','default','The selected modules have been uninstalled.','none'),('949',' modules/system/system.admin.inc:1138','default','The name of this website.','none'),('950',' modules/system/syst