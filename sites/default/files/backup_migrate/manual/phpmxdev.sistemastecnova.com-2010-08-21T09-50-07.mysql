INSERT INTO `phpmx_locales_source` VALUES ('1127',' modules/block/block.admin.inc:161','default','Block title','6.19'),('1128',' modules/block/block.admin.inc:163','default','The title of the block as shown to the user.','6.19'),('1129',' modules/block/block-admin-display-form.tpl.php:42','default','Region','6.19'),('1130',' modules/block/block.module:156','default','Add block','6.19'),('1131',' modules/block/block.admin.inc:241','default','Show if the following PHP code returns <code>TRUE</code> (PHP-mode, experts only).','6.19'),('1132',' modules/block/block.admin.inc:238','default','Enter one page per line as Drupal paths. The \'*\' character is a wildcard. Example paths are %blog for the blog page and %blog-wildcard for every personal blog. %front is the front page.','6.19'),('1133',' modules/block/block.module:140','default','Configure block','6.19'),('1134',' modules/block/block.admin.inc:334','default','Are you sure you want to delete the block %name?','none'),('1135',' modules/block/block.admin.inc:343','default','The block %name has been removed.','none'),('1136',' modules/block/block.admin.inc:213','default','Role specific visibility settings','6.19'),('1137',' modules/block/block.module:315','default','Block description','6.19'),('1138',' modules/block/block.module:121','default','Blocks','6.19'),('1139',' modules/block/block.module:122','default','Configure what block content appears in your site\'s sidebars and other regions.','6.19'),('1140',' modules/block/block.module:148','default','Delete block','6.19'),('1141',' modules/block/block.admin.inc:91','default','Save blocks','6.19'),('1142',' modules/block/block.admin.inc:106','default','The block settings have been updated.','6.19'),('1143',' modules/block/block.admin.inc:156','default','Block specific settings','6.19'),('1144',' modules/block/block.admin.inc:163','default','Override the default title for the block. Use <em>&lt;none&gt;</em> to display no title, or leave blank to use the default block 